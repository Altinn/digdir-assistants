{
  "name": "assistant-crawler",
  "packageManager": "yarn@4.4.1",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "description": "Altinn Assistant crawler",
  "dependencies": {
    "@babel/runtime": "^7.24.7",
    "@crawlee/browser": "*",
    "@crawlee/core": "*",
    "@crawlee/playwright": "*",
    "@crawlee/utils": "*",
    "@digdir/assistant-lib": "1.0.0",
    "commander": "^12.1.0",
    "crawlee": "^3.10.5",
    "playwright": "1.46.1",
    "prettier": "^3.3.2",
    "sha1": "^1.1.1",
    "tiktoken": "^1.0.15",
    "turndown": "^7.2.0",
    "typesense": "^1.8.2"
  },
  "devDependencies": {
    "@playwright/test": "1.46.1",
    "@types/node": "^20.14.9",
    "@types/sha1": "^1.1.5",
    "@types/turndown": "^5.0.4",
    "rimraf": "^6.0.0",
    "tsx": "^4.15.7",
    "typescript": "^5.5.4"
  },
  "scripts": {
    "clean": "rimraf ./dist/",
    "crawl:studio": "tsx src/studio.ts",
    "crawl:infoportal": "tsx src/infoportal.ts",
    "codestyle:check": "prettier src/ --check",
    "codestyle:fix": "prettier src/ --write",
    "build": "cd ../../ && yarn build:assistant-lib && cd services/crawler/ && tsc -p .",
