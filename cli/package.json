{
  "name": "assistants-cli",
  "version": "1.0.0",
  "description": "CLI interface to run tasks and configure Altinn Assistant",
  "main": "main.ts",
  "license": "MIT",
  "private": true,
  "scripts": {
    "clean": "rimraf ./dist/",
    "build": "cd ../ && yarn build:assistant-lib && cd cli/ && tsc -p .",
    "run:generate-search-phrases": "node ./dist/generate-search-phrases/index.js",
    "run:contentAnalysis": "node ./dist/functions/contentAnalysis.js",
    "run:listMissingEnDocs": "node ./dist/functions/listMissingEnDocs.js"
  },
  "dependencies": {
    "@instructor-ai/instructor": "^1.2.1",
    "groq-sdk": "^0.3.3",
    "openai": "^4.33.0",
    "prettier": "^3.2.5",
    "rimraf": "^5.0.5",
    "sha1": "^1.1.1",
    "typescript": "^5.3.3",
    "typesense": "^1.8.2",
    "zod": "^3.22.4"
  }
}
