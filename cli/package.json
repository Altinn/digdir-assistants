{
  "name": "assistants-cli",
  "version": "1.0.0",
  "description": "CLI interface to run tasks and configure Altinn Assistant",
  "main": "main.ts",
  "license": "MIT",
  "private": true,
  "scripts": {
    "clean": "rimraf ./dist/",
    "build": "cd ../ && yarn build:assistant-lib && cd cli/ && tsc -p .",
    "run:generateSearchPhrases": "ts-node ./functions/generateSearchPhrases.ts",
    "run:contentAnalysis": "node ./dist/functions/contentAnalysis.js",
    "run:listMissingEnDocs": "node ./dist/functions/listMissingEnDocs.js",
    "run:listAllDocs": "node ./dist/functions/listAllDocs.js"
  },
  "dependencies": {
    "@digdir/assistant-lib": "*",
    "@types/node": "^20.12.12",
    "@instructor-ai/instructor": "1.2.1",
    "commander": "^12.1.0",
    "groq-sdk": "^0.3.3",
    "openai": "^4.29.1",
    "prettier": "^3.2.5",
    "rimraf": "^5.0.7",
    "sha1": "^1.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
    "typesense": "^1.8.2",
    "zod": "^3.23.8"
  }
}
