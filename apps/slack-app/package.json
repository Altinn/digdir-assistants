    "@digdir/assistant-lib": "1.0.0",
    "@instructor-ai/instructor": "1.2.1",
    "@slack/bolt": "^3.18.0",
    "@slack/web-api": "^6.11.2",
    "@supabase/supabase-js": "^2.39.8",
    "@types/express": "^4.17.21",
    "@types/ramda": "^0.29.12",
    "axios": "^1.6.8",
    "dataclass": "^2.1.1",
    "es-abstract": "^1.23.3",
    "express": "^4.19.2",
    "js-yaml": "^4.1.0",
    "lodash": "^4.17.21",
    "openai": "^4.47.1",
    "prettier": "^3.2.5",
    "ramda": "^0.30.0",
    "remeda": "^1.61.0",
    "typesense": "^1.8.2",
    "zod": "^3.23.8",
    "zod-to-json-schema": "^3.23.0"
  },
  "devDependencies": {
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^20.12.12",
    "@typescript-eslint/eslint-plugin": "^7.10.0",
    "@typescript-eslint/parser": "^7.10.0",
    "eslint": "^9.3.0",
    "eslint-config-prettier": "^9.1.0",
    "nodemon": "^3.1.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "prettier": {
    "endOfLine": "auto",
    "jsxSingleQuote": true,
    "printWidth": 100,
    "singleQuote": true
