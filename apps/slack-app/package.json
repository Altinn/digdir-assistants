{
  "name": "@digdir/assistant-slack-app",
  "packageManager": "yarn@4.9.3",
  "private": true,
  "version": "1.0.0",
  "description": "Slack app implementation of Altinn Assistant",
  "main": "dist/src/app.js",
  "license": "MIT",
  "scripts": {
