    "start": "node dist/src/app.js",
    "go": "yarn run build && yarn start"
  },
  "dependencies": {
    "@azure/openai": "^2.0.0",
    "@babel/runtime": "^7.26.0",
    "@bdb-dd/mack": "https://github.com/bdb-dd/mack.git#14338e54e63d85151e9065d28b25caa778b9a73a",
    "@digdir/assistant-lib": "1.0.0",
    "@instructor-ai/instructor": "1.6.0",
    "@slack/bolt": "^3.21.1",
    "@slack/web-api": "^6.12.1",
    "@supabase/supabase-js": "^2.46.1",
    "@types/ramda": "^0.30.2",
    "axios": "^1.7.7",
    "body-parser": "^1.20.3",
    "dataclass": "^2.1.1",
    "es-abstract": "^1.23.3",
    "express": "^4.21.2",
    "js-yaml": "^4.1.0",
    "lodash": "^4.17.21",
    "openai": "^4.79.0",
    "path-to-regexp": "^6.0.0",
    "prettier": "^3.3.3",
    "ramda": "^0.30.1",
    "remeda": "^2.17.0",
    "typesense": "^1.8.2",
    "zod": "^3.24.1",
    "zod-to-json-schema": "^3.23.5"
  },
  "devDependencies": {
    "@types/body-parser": "^1",
    "@types/express": "^5.0.0",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^22.9.0",
    "@typescript-eslint/eslint-plugin": "^8.13.0",
    "@typescript-eslint/parser": "^8.13.0",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^9.1.0",
    "nodemon": "^3.1.7",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.4"
  },
  "prettier": {
    "endOfLine": "auto",
    "jsxSingleQuote": true,
    "printWidth": 100,
    "singleQuote": true
